<div class="container">
  <div class="row">
    <div class="col-12 p-0">
      <div class="d-flex justify-content-between align-items-center mt-6 mb-5">
        <h3 class="mb-0">Your Team</h3>
        <button id="invite_new_member_btn" class="btn btn-gold float-right">Invite Team Member </button>
      </div>
    </div>
  </div>

  <div class="transactions mb-1">
    <div class="row header">
      <div class="col-3">EMAIL ADDRESS</div>
      <div class="col-3">STATUS </div>

    </div>
  </div>

  <div class="row">
    <div class="col-12 slider-parent-container" data-slider-content-wrapper=".ost-table-row">
      <div id="team_list" class="transactions" data-simple-table data-url="/api/manager/list-admins">
        <%= render :partial => "/web/user_setting/team/team_row" %>
      </div>
    </div>
  </div>
</div>

<% content_for :popups do %>
  <%= render :partial => "/web/user_setting/team/confirm_modals" %>
<%end%>

<% content_for :end_js do %>
  <script type="text/javascript">
    ost.team.init();

    Handlebars.registerHelper('hasVerifiedEmail', function(properties ,  options ) {
      if( properties && properties.indexOf("has_verified_email") > -1  ){
        return options.fn(this);
      }
      return options.inverse(this);
    });

    Handlebars.registerHelper("hasSetupMfa", function(properties ,  options ) {
      if( properties && properties.indexOf("has_setup_mfa") > -1  ){
        return options.fn(this);
      }
      return options.inverse(this);
    });

    Handlebars.registerHelper("isSuperAdmin", function(privilages ,  options ) {
      if( privilages && privilages.indexOf("is_super_admin") > -1  ){
        return options.fn(this);
      }
      return options.inverse(this);
    });

  </script>
<% end %>