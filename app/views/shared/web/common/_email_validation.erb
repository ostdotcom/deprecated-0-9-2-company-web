<%
  email_address = @presenter_obj.manager.email
%>

<div class="email-validation text-center mt-3">
  <svg style="width: 100px">
    <switch>
      <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#email-icon"></use>
    </switch>
  </svg>
  <h3>Confirm your email address</h3>
  <p class="text-muted">We have sent an email to <%= email_address %> please check and confirm your email address to proceed</p>
  <p class="small text-muted">Did not receive an email yet?</p>
  <a href="#" class="btn btn-secondary mt-3">re-send email</a>
  <div class="success" id="successMessage"></div>
  <div class="general_error invalid-feedback">&nbsp;</div>
</div>